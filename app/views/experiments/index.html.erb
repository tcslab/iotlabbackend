<h1>Listing experiments</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Start datetime</th>
      <th>End datetime</th>
      <th>Participation</th>
      <th>Voters</th>
      <th>Ranking</th>
      <th>Interaction</th>
      <th>Status</th>
      <th>Location</th>
      <th>Radius</th>
      <th>Minimum age</th>
      <th>Maximum age</th>
      <th>Gender</th>
      <th>Employment sector</th>
      <th>Employment status</th>
      <th>Camera</th>
      <th>Location</th>
      <th>Accelerometer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiments.each do |experiment| %>
      <tr>
        <td><%= experiment.title %></td>
        <td><%= experiment.description %></td>
        <td><%= experiment.start_datetime %></td>
        <td><%= experiment.end_datetime %></td>
        <td><%= experiment.participation %></td>
        <td><%= experiment.voters %></td>
        <td><%= experiment.ranking %></td>
        <td><%= experiment.interaction %></td>
        <td><%= experiment.status %></td>
        <td><%= experiment.location %></td>
        <td><%= experiment.radius %></td>
        <td><%= experiment.minimum_age %></td>
        <td><%= experiment.maximum_age %></td>
        <td><%= experiment.gender %></td>
        <td><%= experiment.employment_sector %></td>
        <td><%= experiment.employment_status %></td>
        <td><%= experiment.camera %></td>
        <td><%= experiment.location %></td>
        <td><%= experiment.accelerometer %></td>
        <td><%= link_to 'Show', experiment %></td>
        <td><%= link_to 'Edit', edit_experiment_path(experiment) %></td>
        <td><%= link_to 'Destroy', experiment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experiment', new_experiment_path %>
