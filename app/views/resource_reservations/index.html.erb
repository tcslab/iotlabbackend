<h1>Listing resource_reservations</h1>

<table>
  <thead>
    <tr>
      <th>Reservation</th>
      <th>Resource</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resource_reservations.each do |resource_reservation| %>
      <tr>
        <td><%= resource_reservation.reservation %></td>
        <td><%= resource_reservation.resource %></td>
        <td><%= link_to 'Show', resource_reservation %></td>
        <td><%= link_to 'Edit', edit_resource_reservation_path(resource_reservation) %></td>
        <td><%= link_to 'Destroy', resource_reservation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resource reservation', new_resource_reservation_path %>
